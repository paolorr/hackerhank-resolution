{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "strict": "off",
      "func-names": "off",
      "no-plusplus": "off",
      "no-use-before-define": "off"
    }
}